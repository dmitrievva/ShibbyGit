<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui"> 
  <ribbon> 
    <tabs> 
      <tab id="CodeTab" label="ShibbyGit"> 
        <group id="CodeUtils" label="Code Utils"> 
          <button id="ExportAllButton" label="Export All" size="large" onAction="UI.ExportAllMsgBox" imageMso="ExportSavedExports" /> 
          <button id="ImportAllButton" label="Import All" size="large" onAction="UI.ImportAllMsgBox" imageMso="ImportSavedImports" /> 
	  <button id="SetFolderButton" label="Set Folder" size="large" onAction="UI.ShowSetExportDirectoryForm" imageMso="FileManageMenu" /> 
        </group > 
        <group id="Git" label="Git"> 
	  <button id="SetGitPathButton" label="Git Settings" size="large" onAction="UI.ShowGitSettingsForm" imageMso="FileManageMenu" />
          <button id="GitOtherButton" label="Git Console"  onAction="UI.ShowGitOtherForm" imageMso="FileManageMenu" />	  
	  <button id="GitCommitButton" label="Commit" onAction="UI.ShowGitCommitForm" imageMso="FileManageMenu" /> 
	  <button id="GitStatusButton" label="Status" onAction="GitCommands.GitStatus" imageMso="FileManageMenu" />
	  <button id="GitLogButton" label="Log" onAction="GitCommands.GitLog" imageMso="FileManageMenu" />
	  <button id="GitAddAllButton" label="Add All" onAction="GitCommands.GitAddAll" imageMso="FileManageMenu" />
	  <button id="GitRemoteButton" label="Remotes" onAction="UI.ShowGitRemoteForm" imageMso="FileManageMenu" />
        </group > 
      </tab> 
    </tabs> 
  </ribbon> 
</customUI> 